{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/CVS": true,
    "**/.ruby-lsp": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "public": true
  },
  "explorerExclude.backup": {},
  "html.format.enable": false,
  "[html]": {
    "editor.formatOnSave": false
  },
  "spellchecker.language": "en_GB-ise",
  "spellchecker.documentTypes": ["markdown"],
